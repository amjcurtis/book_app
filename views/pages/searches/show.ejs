<!DOCTYPE html>
<html>
    <head></head>
    <body>
        <h1>Search results:</h1>
        <p><a href="/">Home</a></p>
        <ul>
            <% searchResults.forEach(book => { %>
                <img src= <%= book.image_url %>  >
                <li> <%= book.title %> </li> 
                <li> <%= book.author %> </li> 
                <li> <%= book.description %> </li> 
            <% }) %>
        </ul>


    </body>

</html>


